<?xml version="1.0" encoding="utf-8"?><navigation xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:id="@+id/my_nav_host"    app:startDestination="@id/getStarted">    <fragment        android:id="@+id/signUpFragment"        android:name="com.example.gavarstateuniversityapp.fragments.SignUpFragment"        android:label="fragment_sign_up"        tools:layout="@layout/fragment_sign_up" >        <action            android:id="@+id/action_signUpFragment_to_logInFragment"            app:destination="@id/logInFragment" />        <action            android:id="@+id/action_signUpFragment_to_generalFragment"            app:destination="@id/generalFragment" />    </fragment>    <fragment        android:id="@+id/logInFragment"        android:name="com.example.gavarstateuniversityapp.fragments.LogInFragment"        android:label="fragment_log_in"        tools:layout="@layout/fragment_log_in" >        <action            android:id="@+id/action_logInFragment_to_signUpFragment"            app:destination="@id/signUpFragment" />        <action            android:id="@+id/action_logInFragment_to_generalFragment"            app:destination="@id/generalFragment" />    </fragment>    <fragment        android:id="@+id/getStarted"        android:name="com.example.gavarstateuniversityapp.fragments.GetStarted"        android:label="fragment_get_started"        tools:layout="@layout/fragment_get_started" >        <action            android:id="@+id/action_getStarted_to_logInFragment"            app:destination="@id/logInFragment"            app:popUpTo="@id/getStarted"            app:popUpToInclusive="true"            />        <action            android:id="@+id/action_getStarted_to_signUpFragment"            app:destination="@id/signUpFragment"            app:popUpTo="@id/getStarted"            app:popUpToInclusive="true"            />        <action            android:id="@+id/action_getStarted_to_generalFragment"            app:destination="@id/generalFragment"            app:popUpTo="@id/getStarted"            app:popUpToInclusive="true" />    </fragment>    <fragment        android:id="@+id/generalFragment"        android:name="com.example.studentapp.fragments.GeneralFragment"        android:label="fragment_general"        tools:layout="@layout/fragment_general">        <action            android:id="@+id/action_generalFragment_to_quizAnswerFragment"            app:destination="@id/quizAnswerFragment"            app:popUpTo="@id/getStarted"            app:popUpToInclusive="true"            />        <action            android:id="@+id/action_generalFragment_to_logInFragment"            app:destination="@id/logInFragment" />    </fragment>    <fragment        android:id="@+id/quizAnswerFragment"        android:name="com.example.studentapp.accountFragments.QuizAnswerFragment"        android:label="fragment_quiz_answer"        tools:layout="@layout/fragment_quiz_answer">        <argument            android:name="qeusetionsIt"            app:argType="integer"            android:defaultValue="9999" />    </fragment></navigation>